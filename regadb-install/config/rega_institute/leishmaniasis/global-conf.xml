<?xml version="1.0" encoding="UTF-8"?>
<regadb-settings>
 <institute>
   <log-dir>/etc/rega_institute/regadb/leishmaniasis/log</log-dir>
   <import-tool-dir>/etc/rega_institute/regadb/leishmaniasis/import</import-tool-dir>
   <query-result-dir>/etc/rega_institute/regadb/leishmaniasis/queryResult</query-result-dir>
   <files-store-dir>/etc/rega_institute/regadb/leishmaniasis/files</files-store-dir>

   <report-date-tolerance>2</report-date-tolerance>

   <date-format>dd/MM/yyyy</date-format>

   <organism-filter>Leishmaniasis</organism-filter>

   <logo>pics/fiocruz/banner-leish.png</logo>


     <multiple-test-result-forms>
       <form name="1. IDENTIFICACAO" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
         <tests>
	  <test description="Residência Atual" type="Residêcia Atual" organism="Leishmaniasis" showOrganism="false" mandatory = "true"/>
	  <test description="Município que reside" type="Município que reside" organism="Leishmaniasis" showOrganism="false" mandatory = "true"/>
	  <test description="Tempo de Moradia (meses)" type="Tempo de moradia (meses)" organism="Leishmaniasis" showOrganism="false" mandatory = "true"/>
	  <test description="Tipo de Moradia" type="Tipo de Moradia" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Existe alguém que já teve LTA na mesma casa ou região que você mora?" type="Existe alguém que já teve LTA na mesma casa ou região que você mora?" organism="Leishmaniasis" showOrganism="false"/>
       </tests>
      </form>
      <form name="2. ANTECEDENTES MEDICOS" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
         <tests>
          <test description="Está em uso de medicação?" type="Está em uso de medicação?" organism="Leishmaniasis" showOrganism="false" hide="sim;Qual medicaçao estã usando?"/>
	  <test description="Qual medicaçao estã usando?" type="Qual medicação está usando?" organism="Leishmaniasis" showOrganism="false" visible="false" mandatory="true"/>
	  <test description="Diabetes mellitus" type="Diabetes mellitus" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Doença coronariana" type="Doença coronariana" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Chagas" type="Chagas" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Outras doenças" type="Outras doenças" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Qual doença?" type="Qual doença?" organism="Leishmaniasis" showOrganism="false"/>
	  <test description=" Rinite ou sinusite crônica" type=" Rinite ou sinusite crônica" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Já teve LTA" type="Já teve LTA" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Foi tratado (LTA positivo)" type="Foi tratado (LTA positivo)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Natureza do tratamento (LTA positivo)" type="Natureza do tratamento (LTA positivo)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Droga usada e dose (LTA positivo)" type="Droga usada e dose (LTA positivo)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Ano de tratamento (LTA positivo)" type="Ano de tratamento (LTA positivo)" organism="Leishmaniasis" showOrganism="false"/>
 	  <test description="Onde foi tratado (LTA positivo)" type="Onde foi tratado (LTA positivo)" organism="Leishmaniasis" showOrganism="false"/>
	 </tests>
      </form>
      <form name="3. SITUACAO EPIDEMIOLOGICA" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
	<tests>
	  <test description="Número de familiares em casa (Excluir paciente)" type="Número de familiares em casa (Excluir paciente)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Existem animais no quintal" type="Existem animais no quintal" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Galinhas" type="Galinhas" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Cães" type="Cães" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Outros (animais)" type="Outros (animais)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Qual animal (outro)" type="Qual animal (outro)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Renda familiar mensal (sal. Mínimos) " type="Renda familiar mensal (sal. Mínimos) " organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Bolsa-família" type="Bolsa-família" organism="Leishmaniasis" showOrganism="false"/>
	</tests>
      </form>
      <form name="4. EXAME FISICO CUTANEA" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
        <tests>
	  <test description="Suspeita Diagnóstica (SD)" type="Status da Doença (SD)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Número Total de Lesão(ões)" type="Número Total de Lesão(ões)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Lesão maior (cm)" type="Lesão maior (cm)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Lesão maior (área)" type="Lesão maior (área)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Duração (meses)" type="Duração (meses)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização(ões)" type="Localização(ões)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Característica(s) da ferida(s)" type="Característica(s) da ferida(s)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Se lesão ulcerada" type="Se lesão ulcerada" organism="Leishmaniasis" showOrganism="false"/>
          <test description="Foto Cutânea" type="Foto Cutânea" organism="Leishmaniasis" showOrganism="false" />
	<test description="Presença de Linfonodos" type="Presença de Linfonodos" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Caso sim, descreva onde (presença de linfonodos)" type="Caso sim, descreva onde (presença de linfonodos)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Tamanho do gânglio (cm)" type="Tamanho do gânglio (cm)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Observações (Cutânea)" type="Observações (Cutânea)" organism="Leishmaniasis" showOrganism="false"/>
	  </tests>
      </form>
      <form name="5. EXAME FISICO MUCOSA" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
        <tests>
	  
	  <test description="Suspeita Diagnostica (SD) (Mucosa)" type="Suspeita Diagnostica (SD) (Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Lesão Mucosa ativa " type="Lesão Mucosa ativa" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Formas(Lesão Mucosa)" type="Formas(Lesão Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 1 (Lesão Mucosa)" type="Localização 1 (Lesão Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 2 (Lesão Mucosa)" type="Localização 2 (Lesão Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 3 (Lesão Mucosa)" type="Localização 3 (Lesão Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Se nasal(Lesão Mucosa)" type="Se lesão ulcerada" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Exame Físico: Mucosa Nasal" type="Exame Físico: Mucosa Nasal" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 1 (Mucosa nasal)" type="Localização 1 (Mucosa nasal)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 2 (Mucosa nasal)" type="Localização 2 (Mucosa nasal)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 3 (Mucosa nasal)" type="Localização 3 (Mucosa nasal)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description=" Mucosa oral e faringe" type=" Mucosa oral e faringe" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Queixas associadas (Mucosa oral e faringe)" type="Queixas associadas (Mucosa oral e faringe)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Exame Físico: Mucosa oral e faringe" type="Exame Físico: Mucosa oral e faringe" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 1 (Mucosa oral e faringe)" type="Localização 1 (Mucosa oral e faringe)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 2 (Mucosa oral e faringe)" type="Localização 2 (Mucosa oral e faringe)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Localização 3 (Mucosa oral e faringe)" type="Localização 3 (Mucosa oral e faringe)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Outras mucosas (Mucosa oral e faringe)" type="Outras mucosas (Mucosa oral e faringe)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Qual(ais) (Mucosa oral e faringe)" type="Qual(ais) (Mucosa oral e faringe)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Foto" type="Foto" organism="Leishmaniasis" showOrganism="false"/>
	<test description="Presença de Linfonodos (Mucosa)" type="Presença de Linfonodos (Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Caso sim, descreva (presença de linfonodos)(Mucosa)" type="Caso sim, descreva (presença de linfonodos)(Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Tamanho do gânglio (cm) (Mucosa)" type="Tamanho do gânglio (cm) (Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Observações (Mucosa)" type="Observações (Mucosa)" organism="Leishmaniasis" showOrganism="false"/>
	  </tests>
      </form>
      <form name="6. COLETA" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
        <tests>
	  <test description="Biópsia" type="Biópsia" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Se sim, descreva (Biópsia)" type="Se sim, descreva (Biópsia)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Sangue" type="Sangue" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Se sim, descreva (Sangue)" type="Se sim, descreva (Sangue)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Aspirado de linfonodo" type="Aspirado de linfonodo" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Se sim, descreva (Aspirado de linfonodo)" type="Se sim, descreva (Aspirado de linfonodo)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Swab da lesão" type="Swab da lesão" organism="Leishmaniasis" showOrganism="false"/>  
	  <test description="Se sim, descreva (Swab da lesão)" type="Se sim, descreva (Swab da lesão)" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Swab nasal" type="Swab nasal" organism="Leishmaniasis" showOrganism="false"/>  
	  <test description="Se sim, descreva (Swab nasal)" type="Se sim, descreva (Swab nasal)" organism="Leishmaniasis" showOrganism="false"/>
        </tests>
      </form>
      <form name="7. DIAGNOSTICO LABORATORIAL" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
       <tests> 
	<test description="IFI" type="IFI" organism="Leishmaniasis" showOrganism="false"/>
