      </tests>
     </form>
     <form name="8. DIAGNOSTICO LEISHMANIA" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
      <tests>
	<test description="IDRM (mm)" type="IDRM (mm)" organism="Leishmaniasis" showOrganism="false"/>
	<test description="ELISA (Anti Leishmania)" type="ELISA (Anti Leishmania)" organism="Leishmaniasis" showOrganism="false"/>
