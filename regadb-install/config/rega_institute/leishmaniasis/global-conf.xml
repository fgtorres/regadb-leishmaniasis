     <form name="9. ACOMPANHAMENTO" gridRowCount="1" todayDefaultContactDate="true" showSampleId="false">
      <tests>
	  <test description="Status da Lesão" type="Status da Lesão" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Nível Quantitativo das Lesões" type="Nível Quantitativo das Lesões" organism="Leishmaniasis" showOrganism="false"/>
	  <test description="Status do tratamento" type="Status do tratamento" organism="Leishmaniasis" showOrganism="false"/>
